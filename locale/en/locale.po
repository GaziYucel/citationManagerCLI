msgid "plugins.importexport.citationManagerCLI.displayName"
msgstr "Citation Manager Plugin CLI"

msgid "plugins.importexport.citationManagerCLI.description"
msgstr "Command Line Interface for the Citation Manager Plugin batch process and deposit."

msgid "plugins.importexport.citationManagerCLI.cliError"
msgstr "ERROR"

msgid "plugins.importexport.citationManagerCLI.cliUsage"
msgstr ""
"Usage: \n"
"{$scriptName} {$pluginName} process\n"
"{$scriptName} {$pluginName} deposit\n"
"This plugin needs the generic plugin Citation Manager\n"
""

msgid "plugins.importexport.CitationManagerCLIPlugin.notInstalled"
msgstr "The generic Citation Manager Plugin is not installed"

msgid "plugins.importexport.citationManagerCLI.descriptionExtendedHtml"
msgstr ""
"Command Line Interface for the Citation Manager Plugin.<br>"
"<br>"
"This plugin can only be executed from the command line.<br>"
"<br>"
"Execute the batch process with the following command:"
"<pre>php $OJS/tools/importExport.php {$pluginName} process</pre>"
"Execute the batch deposit with the following command: <br>"
"<pre>php $OJS/tools/importExport.php {$pluginName} deposit</pre>"
"Notes"
"<br>"
"This plugin needs the generic plugin Citation Manager and will not work without.<br>"
"The Citation Manager can be downloaded from: "
"<a href='https://github.com/TIBHannover/citationManager' target='_blank'>https://github.com/TIBHannover/citationManager</a><br>"
